{
  "name": "@sudosos/sudosos-client",
  "description": "Frontend client for the SudoSOS barsystem",
  "version": "1.1.2",
  "private": true,
  "main": "dist/index.js",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "tsc",
    "gen-build": "npm run gen && npm run build && (git add ./src && git add ./dist)",
    "gen": "openapi-generator-cli generate -i swagger.json  -g typescript-axios -o ./src --openapi-normalizer REFACTOR_ALLOF_WITH_PROPERTIES_ONLY=true",
    "update-swagger": "cd ./node_modules/@sudosos/sudosos-backend && npx npm install && npx npm run swagger && cd ../../../ &&  cp ./node_modules/@sudosos/sudosos-backend/out/swagger.json ./swagger.json"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/node": "^18.7.7",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "@sudosos/sudosos-backend": "github:GEWIS/sudosos-backend#main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GEWIS/sudosos-client.git"
  },
  "files": [
    "src/",
    "dist/",
    "index.js",
    "swagger.json"
  ],
  "author": "SudoSOS WG",
  "bugs": {
    "url": "https://github.com/GEWIS/sudosos-client/issues"
  },
  "homepage": "https://github.com/GEWIS/sudosos-client#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "swagger-axios-codegen": "^0.13.3"
  }
}
